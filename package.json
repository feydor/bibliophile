    "express-session": "^1.17.1",
    "express-validator": "^6.9.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "pug": "2.0.0-beta11",
    "sinon": "^9.2.4"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
