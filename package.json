{
  "name": "bibliophile",
  "version": "0.0.0",
  "description": "An Express.js app to keep track of personal libraries and reccomendations.",
  "private": true,
  "scripts": {
    "sass": "sass public/stylesheets/main.scss public/stylesheets/main.css",
    "start": "node ./bin/www",
